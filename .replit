modules = ["nodejs-18"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["jq", "openjdk11"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Firebase Backend"

[[workflows.workflow]]
name = "Firebase Backend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "firebase emulators:start --only functions,firestore,auth,storage,ui"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

